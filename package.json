{
  "name": "@salier1/elephant-design",
  "version": "0.5.0",
  "private": false,
  "description": "haha",
  "keywords": [
    "toolkit",
    "rollup",
    "typescript"
  ],
  "author": "hejiawei",
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.js",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/salier1/elephant-design"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/classnames": "^2.3.1",
    "async-validator": "^4.2.5",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "react-transition-group": "^4.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "build": "npm run clean && npm run build-es && npm run build-umd",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "prepublishOnly": "npm run test:nowatch && npm run lint && npm run build",
    "@types/react": "^18.0.8",
    "storybook": "BROWSER=none start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-es": "rollup --config rollup/rollup.esm.config.js",
    "build-umd": "rollup --config rollup/rollup.umd.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && npm run lint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)",
      "<rootDir>/node_modules/(?!axios)"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-flow": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@chromatic-com/storybook": "^1.3.4",
    "@craco/craco": "^7.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/addon-onboarding": "^8.0.10",
    "@storybook/blocks": "^8.0.10",
    "@storybook/preset-create-react-app": "^8.0.10",
    "@storybook/react": "^8.0.10",
    "@storybook/react-webpack5": "^8.0.10",
    "@storybook/test": "^8.0.10",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^16.18.96",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-transition-group": "^4.4.10",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "node-sass": "^7.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
    "rollup-plugin-sass": "^1.12.22",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.76.0",
    "storybook": "^8.0.10",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "webpack": "^5.91.0"
  }
}
